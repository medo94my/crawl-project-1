# Flask settings
FLASK_ENV=development
FLASK_APP=app.py
FLASK_DEBUG=1

# Crawl4AI settings (optional, if you later configure API keys or custom settings)
CRAWL4AI_USER_AGENT=Mozilla/5.0 (compatible; MyCrawler/1.0; +http://example.com)

# App secrets (if you use session or forms with Flask)
SECRET_KEY=your_secret_key_here

# Optional: Custom output folder for crawl results
CRAWL_RESULTS_DIR=results/

# Optional: Timeout for the crawler (in seconds)
CRAWL_TIMEOUT=10
GEMINI_API_KEY=Your_Gemini_API_Key
GROQ_API_KEY=Your_GROQ_API_Key
